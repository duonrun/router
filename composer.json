{
	"name": "duon/router",
	"description": "PSR-7/PSR-15 router and dispatcher",
	"type": "library",
	"license": "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Duon\\Router\\": "src/"
		},
		"files": [
			"src/Utils.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Duon\\Router\\Tests\\": "tests/"
		},
		"files": [
			"tests/Fixtures/globalTestSymbols.php"
		]
	},
	"authors": [
		{
			"name": "ebene fÃ¼nf GmbH",
			"email": "info@ebenefuenf.de"
		}
	],
	"require": {
		"php": ">=8.3",
		"duon/wire": "dev-main",
		"psr/container": "^2.0",
		"psr/http-message": "^1.1 || ^2.0",
		"psr/http-message-implementation": "^1.0",
		"psr/http-server-handler": "^1.0",
		"psr/http-server-middleware": "^1.0"
	},
	"require-dev": {
		"duon/dev": "dev-main",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0",
		"laminas/laminas-diactoros": "^3.3",
		"phpunit/phpunit": "^12.0",
		"vimeo/psalm": "^6.10"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
